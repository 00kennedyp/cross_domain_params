___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const getUrl = require('getUrl');
const cookie = require('setCookie');

const querystring = getUrl('query');
const query_params = data.query_params.split(',');

// Set the max-age for cookies
const maxAge = data.cookie_max_age; // 30 days

// Function to set cookies based on query parameters
for (var index = 0; index < query_params.length; index++) {
  const param = query_params[index];
  const regex = param + "=([0-9a-zA-Z_]+)";
  const match = querystring.match(regex);

  if (match && match[1]) { // Ensure match is not null and has a capturing group
    cookie(param, match[1], {
      domain: 'auto',
      path: '/',
      "max-age": maxAge,
    });
  }
}


data.gtmOnSuccess();


